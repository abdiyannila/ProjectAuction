@model IEnumerable<Auction2.Models.BidUserModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
<thead class="thead-inverse">
    <tr>
        <th>@Html.DisplayNameFor(m => m.Id)</th>
        <th>@Html.DisplayNameFor(m => m.Nama)</th>
        <th>@Html.DisplayNameFor(m => m.Harga)</th>
        <th>@Html.DisplayNameFor(m => m.TanggalBid)</th>
        <th>@Html.DisplayNameFor(m => m.BatasBid)</th>
        <th>@Html.DisplayNameFor(m => m.MerkName)</th>
        <th>Modify</th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.Id)</td>
            <td>@Html.DisplayFor(m => item.Nama)</td>
            <td>@Html.DisplayFor(m => item.Harga)</td>
            <td>@Html.DisplayFor(m => item.TanggalBid)</td>
            <td>@Html.DisplayFor(m => item.BatasBid)</td>
            <td>@Html.DisplayFor(m => item.MerkName)</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>

    }
</tbody>
</table>

@Html.ActionLink("Create New Bid", "Create")

